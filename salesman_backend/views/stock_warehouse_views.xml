<odoo>
    <record id="view_warehouse_form_inherit_salesman" model="ir.ui.view">
        <field name="name">stock.warehouse.form.salesman</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.warehouse_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='code']" position="after">
                <field name="salesman_ids"/>
            </xpath>
        </field>
    </record>
</odoo>
